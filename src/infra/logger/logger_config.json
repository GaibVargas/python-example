{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "json": {
            "class": "pythonjsonlogger.jsonlogger.JsonFormatter",
            "format": "%(asctime)s %(levelname)s %(name)s %(module)s %(process)s %(thread)s %(message)s",
            "datefmt": "%Y-%m-%dT%H:%M:%S%z"
        }
    },
    "handlers": {
        "stdout": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "json",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "uvicorn": {
            "handlers": [],
            "level": "INFO",
            "propagate": true
        },
        "fastapi": {
            "handlers": [],
            "level": "INFO",
            "propagate": true
        },
        "sqlalchemy": {
            "handlers": [],
            "level": "WARNING",
            "propagate": true
        },
        "apscheduler": {
            "handlers": [],
            "level": "WARNING",
            "propagate": true
        }
    },
    "root": {
        "level": "DEBUG",
        "handlers": [
            "stdout"
        ]
    }
}